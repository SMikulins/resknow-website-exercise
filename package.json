{
  "name": "resknow-website-exercise",
  "author": "Sergejs Mikulins",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/style.css -o ./dist/css/style.css",
    "watch": "npx tailwindcss -i ./src/style.css -o ./dist/css/style.css --watch",
    "production": "cross-env NODE_ENV=production npx tailwindcss -i ./src/style.css -o ./dist/css/style.css --minify",
    "prettier": "npx prettier --write ./dist/*.html"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3"
  }
}
